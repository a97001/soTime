{
  "name": "soTime-api",
  "version": "0.1.0",
  "description": "A Boilerplate application for building REST APIs using express, mongoose in ES6 with code coverage",
  "author": "Kunal Kapadia <kunalkapadia12@gmail.com>",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": "6.2.x",
    "npm": "3.9.x"
  },
  "scripts": {
    "start": "gulp serve",
    "build": "gulp",
    "lint": "gulp lint",
    "test": "gulp mocha",
    "commit": "git-cz",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:KunalKapadia/express-mongoose-es6-rest-api.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API",
    "boilerplate"
  ],
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "bluebird": "^3.1.4",
    "body-parser": "^1.14.2",
    "co": "^4.6.0",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "cors": "^2.7.1",
    "crypto": "0.0.3",
    "debug": "^2.2.0",
    "eslint": "^2.13.1",
    "eslint-plugin-import": "^1.9.2",
    "express": "~4.13.1",
    "express-jwt": "^3.4.0",
    "express-validation": "^0.4.5",
    "express-winston": "^1.2.0",
    "geoip-lite": "^1.1.8",
    "gridfs-stream": "^1.1.1",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "http-status": "^0.2.0",
    "joi": "^7.2.3",
    "joi-objectid": "^2.0.0",
    "jsonwebtoken": "^7.0.1",
    "lodash": "^4.13.1",
    "method-override": "^2.3.5",
    "moment": "^2.13.0",
    "mongoose": "^4.3.7",
    "morgan": "~1.6.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "sharp": "^0.15.1",
    "socket.io": "^1.4.6",
    "socket.io-adapter-mongo": "^0.1.6",
    "socketio-jwt": "^4.4.0",
    "ua-parser-js": "^0.7.10",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.9.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.4.1",
    "commitizen": "2.5.0",
    "coveralls": "^2.11.6",
    "cz-conventional-changelog": "1.1.5",
    "del": "^2.2.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "ghooks": "1.0.3",
    "gulp": "^3.9.0",
    "gulp-apidoc": "^0.2.4",
    "gulp-babel": "6.1.2",
    "gulp-env": "^0.4.0",
    "gulp-eslint": "^2.0.0",
    "gulp-istanbul": "^0.10.3",
    "gulp-load-plugins": "^1.2.0",
    "gulp-mocha": "^2.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-plumber": "^1.0.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4",
    "run-sequence": "^1.1.5",
    "socket.io-client": "^1.4.6",
    "supertest": "^1.1.0",
    "supertest-as-promised": "^2.0.2",
    "validate-commit-msg": "2.0.0"
  },
  "license": "MIT",
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint && npm test",
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "add-module-exports",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  }
}
